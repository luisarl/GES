{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.1.1",
        "doctrine/dbal": "^3.8.3",
        "guzzlehttp/guzzle": "^7.8.1",
        "intervention/image": "^2.7.2",
        "karriere/pdf-merge": "^2.1",
        "laravel/framework": "^10.48.7",
        "laravel/sanctum": "^3.3.3",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.5.1",
        "maatwebsite/excel": "^3.1.55",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/ignition": "^1.13.2",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13.4",
        "fakerphp/faker": "^1.23.1",
        "kitloong/laravel-migrations-generator": "^6.11.1",
        "laravel/sail": "^1.29.1",
        "mockery/mockery": "^1.6.11",
        "nunomaduro/collision": "^7.10",
        "phpunit/phpunit": "^10.5.18",
        "spatie/laravel-ignition": "^2.5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
